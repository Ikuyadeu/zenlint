
{
    "problemMatcher": [
        {
            "owner": "zenlint",
            "pattern": [
                {
                    "regexp": "^(warning|error)\\s+(Available|Ignored)\\s+(.+)",
                    "severity": 1,
                    "message": 2,
                    "code": 3
                }
            ]
        }
    ]
}