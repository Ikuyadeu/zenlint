env:
  node: true
globals: {}
parser: >-
  /Users/yuki-ud/Developer/app/linter-maintainer/node_modules/@typescript-eslint/parser/dist/index.js
parserOptions:
  ecmaVersion: 12
  sourceType: module
  project: ./tsconfig.json
plugins:
  - jsdoc
  - '@typescript-eslint'
rules:
